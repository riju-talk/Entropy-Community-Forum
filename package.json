{
  "name": "Entropy-Community",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "postbuild": "npm run docker:build:ai",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "docker:build:ai": "docker build -t entropy/ai-agent:latest -f apps/ai-agent/Dockerfile apps/ai-agent",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "npm --workspace apps/tests run test",
    "test:watch": "npm --workspace apps/tests run test:watch"
  },
  "dependencies": {
    "@auth/core": "latest",
