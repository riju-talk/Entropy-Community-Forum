{
  "version": 2,
  "framework": "nextjs",
  "installCommand": "cd apps/app && npm ci",
  "buildCommand": "cd apps/app && npm run build",
  "outputDirectory": "apps/app/.next",
  "ignoreCommand": "echo 'Ignoring non-frontend workspaces (ai-agent)'",
  "routes": [
    {
      "src": "/api/ai-agent/(.*)",
      "dest": "/app/api/ai-agent/$1"
    }
  ]
}
